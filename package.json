{
	"name": "ndjson-to-observable",
	"version": "1.0.2",
	"description": "Create an observable stream from a ndjson stream",
	"repository": "https://github.com/SimonJang/ndjson-to-observable",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"scripts": {
		"lint": "tslint -p . --format stylish",
		"format": "prettier --write \"source/**/*.ts\"",
		"build": "npm run clean && tsc -p tsconfig.json",
		"test": "npm run lint && npm run format && ava",
		"clean": "del-cli lib",
		"prepare": "npm run test && npm run build",
		"deploy": "npm run prepare && npx np"
	},
	"engines": {
		"node": ">=8"
	},
	"keywords": [
		"streams",
		"ndjson",
		"observable",
		"rxjs",
		"io"
	],
	"files": [
		"lib"
	],
	"author": {
		"name": "Simon Jang",
		"email": "simon.jang.contact@gmail.com"
	},
	"license": "MIT",
	"dependencies": {
		"@samverschueren/stream-to-observable": "^0.3.0",
		"split": "^1.0.1",
		"strip-bom-stream": "^4.0.0"
	},
	"devDependencies": {
		"@types/node": "^9.6.55",
		"ava": "^3.9.0",
		"del-cli": "^0.2.1",
		"prettier": "^1.19.1",
		"rxjs": "^6.5.3",
		"ts-node": "^8.5.2",
		"tsd": "^0.11.0",
		"tslint": "^5.20.1",
		"tslint-config-prettier": "^1.18.0",
		"tslint-xo": "^0.7.2",
		"type-fest": "^0.8.0",
		"typescript": "^3.7.2"
	},
	"ava": {
		"files": [
			"source/test/**/*"
		],
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	},
	"peerDependencies": {
		"rxjs": "^6.5.3"
	}
}
